<!-- LOGIN -->
    <section class="module">
      <div class="container">

        <div class="row">
          <div class="col-sm-4 col-sm-offset-4">

           <h2><%= t('.sign_in', :default => "Sign in") %></h2>

            <%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
              <div><%= f.label :email %><br />
              <%= f.email_field :email, class: "form-control input-lg" %></div>

               <div><%= f.label :password %><br />
              <%= f.password_field :password, class: "form-control input-lg"  %></div>

              <% if devise_mapping.rememberable? -%>
                <div><%= f.check_box :remember_me %> <%= f.label :remember_me %></div>
              <% end -%>
              <br />

              <div><%= f.submit t('.sign_in', :default => "Sign in"), class: "btn btn-block btn-round btn-base" %></div>
            <% end %>
          <%= render "devise/shared/links" %>
          </div>
        </div>

      </div>
    </section>
    <!-- END LOGIN


